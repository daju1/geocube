// 2 input units
-i=2

// 1 output units
-o=1

// 1 hidden layer
-hh=1

// 1st hidden layer = 2 units
-h=2

// use demo1.rgn as recognition file
-frecog=demo1.rgn

// and there are 2 recognition patterns
-samp=2

// use demo1.out as output file
-fout=demo1.out
